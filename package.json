{
  "name": "epic-application",
  "version": "2.0.1",
  "description": "A powerful express based api framework.",
  "private": true,
  "packageManager": "npm@8.19.1",
  "engines": {
    "node": "16.15.0"
  },
  "epic": {
    "branch": "v2",
    "locals": {},
    "plugins": {},
    "packingSources": [
      "build/**",
      "resources/**",
      "package.json",
      "epic.lock.json"
    ]
  },
  "scripts": {
    "dev": "npx -y cross-env NODE_ENV=development nodemon --ignore 'cache/' --files ./src/core/index.ts",
    "start": "npx -y cross-env NODE_ENV=production node ./build/core/index.js",
    "build": "epic build -c",
    "test": "jest --runInBand",
    "test:watch": "jest --watch --runInBand",
    "test:coverage": "jest --coverage --runInBand",
    "lint:check": "eslint .",
    "lint:fix": "eslint . --fix",
    "format:prettier": "prettier . --write",
    "sync:postman": "npx -y cross-env NODE_ENV=development ts-node --ignore 'cache/' -T --files ./src/core/syncPostman.ts",
    "sync:db": "npx -y cross-env NODE_ENV=production ts-node --ignore 'cache/' -T --files ./src/core/syncDatabase.ts"
  },
  "dependencies": {
    "@oridune/epic-odm": "^1.1.1",
    "@oridune/validator": "^0.0.21",
    "@saffellikhan/epic-express": "^2.0.26",
    "@saffellikhan/epic-schedular": "^2.0.3",
    "@saffellikhan/epic-time": "^1.0.0",
    "@supercharge/request-ip": "^1.1.2",
    "@types/cors": "^2.8.12",
    "compose-middleware": "^5.0.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "dotenv": "^10.0.0",
    "epic-geo": "^2.0.6",
    "epic-tokens": "^3.0.5",
    "epic-translate": "^1.0.9",
    "express-rate-limit": "^6.4.0",
    "express-useragent": "^1.0.15",
    "glob": "^7.1.6",
    "helmet": "^4.2.0",
    "hpp": "^0.2.3",
    "http-status": "^1.5.2",
    "ioredis": "^5.0.5",
    "ip-filter": "^3.0.5",
    "mongodb": "^4.3.0",
    "morgan": "^1.10.0",
    "slug": "^4.0.2"
  },
  "devDependencies": {
    "@types/compression": "^1.7.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express-useragent": "^1.0.1",
    "@types/glob": "^7.1.3",
    "@types/hpp": "^0.2.2",
    "@types/jest": "^28.1.3",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.7",
    "@types/slug": "^0.9.1",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "eslint": "^8.18.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.1.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.5.3",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "husky": "^8.0.0",
    "jest": "^28.1.1",
    "lint-staged": "^13.0.3",
    "nodemon": "^2.0.7",
    "prettier": "^2.7.1",
    "supertest": "^6.2.3",
    "ts-jest": "^28.0.5",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.14.1",
    "typescript": "^4.0.5"
  },
  "keywords": [
    "epic",
    "server",
    "backend",
    "application",
    "api"
  ],
  "author": "Saif Ali Khan",
  "license": "MIT",
  "homepage": "https://github.com/Saff-Elli-Khan/epic-application#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Saff-Elli-Khan/epic-application.git"
  },
  "bugs": {
    "url": "https://github.com/Saff-Elli-Khan/epic-application/issues"
  },
  "lint-staged": {
    "!(build/**/*)*.{js,ts}": "eslint --cache --fix",
    "!(build/**/*)*.{js,ts,json,md}": "prettier --write"
  }
}
