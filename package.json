{
  "name": "epic-application",
  "version": "1.0.0",
  "description": "A powerful Express Application Server Framework.",
  "scripts": {
    "start": "tsc && cross-env PORT=8080 NODE_ENV=production node ./build/App.js",
    "dev": "cross-env PORT=8080 NODE_ENV=development nodemon --ignore 'cache/' -T --files ./src/App.ts",
    "gcloud-deploy": "gcloud config set project {project_id} && gcloud app deploy -q"
  },
