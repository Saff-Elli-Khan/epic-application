    "cross-env": "^7.0.2",
    "dotenv": "^10.0.0",
    "epic-config-manager": "^1.0.3",
    "epic-geo": "^2.0.6",
    "epic-money": "^1.0.12",
    "epic-pay": "^1.0.13",
    "epic-permissions-manager": "^2.0.11",
    "epic-tokens": "^1.0.10",
    "epic-validator": "^2.0.7",
    "express": "^4.17.1",
